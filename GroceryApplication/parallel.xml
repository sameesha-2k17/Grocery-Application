<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="parallel" parallel="methods">
<!-- for report gen when the test cases are executed-->
<listeners>
 <listener class-name="reportgeneration.Listener">
  </listener>
 </listeners>
 
 <!--same testcases run in multiple times with same browser-->
  <parameter name="browser" value="chrome"></parameter>
  <test thread-count="5" name="Test1">
    <classes>
     <!-- <class name="testscript.HomeTest"/>
      <class name="testscript.ManageNewsTest"/>
      <class name="testscript.AdminTest"/>
      <class name="testscript.LoginTest"/>-->
    <class name="testscript.LoginTest">
      <methods> 
      <include name="invalidusernameInvalidpassword2"></include>
      </methods>
      </class>
       </classes>    
  </test> <!-- Test -->
  
  <test thread-count="5" name="Test2">
    <classes>
     <!-- <class name="testscript.HomeTest"/>
      <class name="testscript.ManageNewsTest"/>
      <class name="testscript.AdminTest"/>
      <class name="testscript.LoginTest"/>-->
    <class name="testscript.LoginTest">
      <methods> 
      <include name="invalidusernameInvalidpassword2"></include>
      </methods>
      </class>
       </classes>    
  </test> <!-- Test -->
</suite> <!-- Suite -->
